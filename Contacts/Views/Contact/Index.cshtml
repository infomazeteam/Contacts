@model ContactService.ContactModel
@{
    ViewData["Title"] = "Contact";
}
<style>
    .Width-200{
        width:200px;
    }
</style>
<div class="content">
    <div class="tab-content">
        <div class="tab-pane active">
            <div class="panel panel-flat no-margin">
                <div class="pull-right">
                </div>
                <div class="table-responsive">
                    <table class="table table-hover" id="contactgrid">
                        <thead>
                            <tr>
                                <th class="text-center">Action</th>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Email</th>
                                <th>Mobile</th>
                                <th>Phone</th>
                                <th>Address1</th>
                                <th>Address2</th>
                                <th>City</th>
                                <th>State</th>
                                <th>Zip</th>
                                <th>Country</th>
                          </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    <script src="~/js/pages/datatables_basic.js" asp-append-version="true"></script>
    <script src="~/js/tables/datatables/datatables.min.js" asp-append-version="true"></script>
    <script src="~/js/contact.js" asp-append-version="true"></script>
    <script>
        $(document).ready(function () {
            LoadContactList();
        });
    </script>
}
